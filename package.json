        "generate:prod": "cross-env NODE_ENV='prod' nuxt-ts generate",
        "test": "jest"
    },
    "dependencies": {
        "@nuxt/cli-edge": "^2.17.2-28284499.5674f49",
        "@nuxt/typescript-runtime": "^2.0.0",
        "@nuxtjs/axios": "^5.12.2",
        "@nuxtjs/google-adsense": "^1.1.3",
        "@nuxtjs/sitemap": "^2.4.0",
        "bulma": "^1.0.1",
        "cross-env": "^7.0.2",
        "jstat": "^1.9.3",
        "nuxt": "^2.14.4",
        "sass": "^1.77.4"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@nuxt/types": "^2.14.4",
        "@nuxt/typescript-build": "^2.0.3",
        "@nuxtjs/eslint-config-typescript": "^3.0.0",
        "@nuxtjs/style-resources": "^1.0.0",
        "@vue/test-utils": "^1.0.5",
