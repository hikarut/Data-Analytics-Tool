        "babel-jest": "^26.3.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-vue": "^6.2.2",
        "jest": "^26.4.2",
        "kind-of": ">=6.0.3",
        "lodash": ">=4.17.19",
        "minimist": ">=1.2.3",
        "nodemon": "^2.0.4",
        "prettier": "2.1.1",
        "sass-loader": "^10.0.2",
        "vue-jest": "^3.0.6",
        "vue-lazyload": "^1.3.3"
    },
    "jest": {
        "transform": {
            "^.+\\.js$": "babel-jest",
            ".*\\.(vue)$": "vue-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/$1",
            "^@@/(.*)$": "<rootDir>/$1",
            "^~/(.*)$": "<rootDir>/$1",
            "^~~/(.*)$": "<rootDir>/$1"
        },
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "**/*.{js,vue}",
            "!**/node_modules/**"
        ]
    },
    "babel": {
        "env": {
            "test": {
                "presets": [
                    "@babel/preset-env"
                ]
            }
        }
    }
}
