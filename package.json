    "description": "Data-Analytics-Tool",
    "author": "hikarut",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV='dev' nuxt-ts",
        "preview": "cross-env NODE_ENV='preview' nuxt-ts",
        "prod": "cross-env NODE_ENV='prod' nuxt-ts",
        "build": "nuxt-ts build",
        "start": "nuxt-ts start",
        "generate:dev": "cross-env NODE_ENV='dev' nuxt-ts generate",
